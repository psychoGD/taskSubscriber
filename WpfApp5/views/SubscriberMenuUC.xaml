<UserControl x:Class="WpfApp5.views.SubscriberMenuUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfApp5.views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="White">
        <Button VerticalAlignment="Top" 
                HorizontalAlignment="Left" 
                Width="50" Height="50" 
                Content="Back" 
                Command="{Binding BackCommand}"></Button>
        <StackPanel>
            <Button Width="200" Height="50" Content="Sign In" Margin="0,50,0,50"></Button>
            <Button Width="200" Height="50" Content="Register" 
                    Command="{Binding RegisterCommand}"></Button>
        </StackPanel>  
    </Grid>
</UserControl>
